// 进度条
.progress
  height: 6px;
  line-height: 6px;
  background: #1B302D;
  box-shadow: none;
  margin:0;
  overflow: visible;
  border-radius: 5px;
  width:260px;
  position: relative;
  transition: transform 0.2s
  cursor pointer
  margin: 33px 0 42px 113px
  &:hover
    span:nth-child(1)
      color: blue!important
    span,span.orange
      text-shadow:0px 0px 3px #888;
  .progress-bar
    border-radius: 5px;
    animation: animate-positive 3s;
    background-image: linear-gradient(90deg, #1DAB83, #4AF9D1);
    float: left;
    width: 0;
    height: 100%;
    font-size: 12px;
    line-height: 20px;
    color: #fff;
    text-align: center;
    transition: transform 0.2s
    &:hover
      box-shadow: 0 0 15px #888888;
      transform: scale(1, 1.3)
  .progress-bar.orange
    background: orange
   
  span
    position: absolute;
    top: -19px
    display: block;
    font-size: 14px;
  span.orange
    color orange!important
  span:nth-child(1)
    top: 0
    left: -55px;
    color: #356670;
    letter-spacing: 0.03px;
  span:nth-child(2)
    color: #AFC3DD;
    right: 0;
    font-size: 14px;
    i 
      font-family: myFirstFont;
      font-size: 28px;
      color: #4AF9D1;
      margin-right: 5px;
@-webkit-keyframes animate-positive {
  0% {
    width: 0%;
  }
}
@keyframes animate-positive {
  0% {
    width:0%;
  }
}
@keyframes fade {
    from { box-shadow: 0 0 11px 0 rgba(251,87,48,0) }
    50% { box-shadow: 0 0 11px 0 rgba(251,87,48,0.84), 0 0 11px 0 rgba(251,87,48,0.84) }
    to {box-shadow: 0 0 11px 0 rgba(251,87,48,0) }
}
 
@-webkit-keyframes fade {
    from { box-shadow: 0 0 11px 0 rgba(251,87,48,0) }
    50% { box-shadow: 0 0 11px 0 rgba(251,87,48,0.84), 0 0 11px 0 rgba(251,87,48,0.84)}
    to { box-shadow: 0 0 11px 0 rgba(251,87,48,0.2) }
}
